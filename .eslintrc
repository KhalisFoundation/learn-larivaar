{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jquery": true
  },
  "rules": {
    "no-mixed-operators": "off",
    "no-restricted-globals": "off",
    "no-use-before-define": "off",
    "react/jsx-filename-extension": 0
  },
  "globals": {
    "cordova": true,
    "ga": true,
    "Hammer": true,
    "Materialize": true,
    "StatusBar": true
  }
}
